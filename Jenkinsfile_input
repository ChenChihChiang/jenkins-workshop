pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
      	script{
	      input message: 'https://abcde', parameters: [choice(choices: ['MAC', 'Linux', 'Windows'], description: 'OS platform', name: 'PLATFORM')]
	      echo "Hello, ${PLATFORM}, nice to meet you."
		}
      }
    }
  }
}
