pipeline {
  agent any
  
  stages {
    stage('Hello') {
      steps {
      	script{
	  platform = input message: 'https://abcde', parameters: [choice(choices: ['MAC', 'Linux', 'Windows'], description: 'OS platform', name: 'pppp')]
          sh label: '', script: 'env'
	  echo "Hello, ${platform}, nice to meet you."
	}
      }
    }
  }
}
